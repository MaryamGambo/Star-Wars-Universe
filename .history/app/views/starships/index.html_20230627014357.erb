<p style="color: green"><%= notice %></p>

<h1>Starships</h1>
<%= paginate @starships%>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Model</th>
    <th>Manufacturer</th>
    <th>Actions</th>
  </tr>
  <% @starships.each do |starship| %>
    <%= render partial: "starship", locals: { starship: starship } %>
  <% end %>
</table>

<%= link_to "New starship", new_starship_path %>
